{
    "name": "yaliniyalu/monday_cs",
    "description": "Customer Support app for monday.com",
    "keywords": [
        "monday.com",
        "customer support"
    ],
    "homepage": "https://github.com/yaliniyalu",
    "license": "MIT",
    "authors": [
        {
            "name": "Samar Yalini",
            "email": "samarjben@outlook.com",
            "homepage": "https://github.com/yaliniyalu"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "cboden/ratchet": "^0.4.3",
        "google/apiclient": "^2.8",
        "google/auth": "^1.14",
        "guzzlehttp/guzzle": "7.0",
        "lcobucci/jwt": "^3.3",
        "league/oauth2-client": "^2.6",
        "league/oauth2-google": "^3.0",
        "microsoft/microsoft-graph": "^1.24",
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.1",
        "slim/psr7": "^1.1",
        "slim/slim": "^4.5",
        "slim/twig-view": "3.0",
        "spomky-labs/base64url": "^2.0",
        "stevenmaguire/oauth2-microsoft": "^2.2",
        "textalk/websocket": "^1.4",
        "thingengineer/mysqli-database-class": "dev-master",
        "vlucas/phpdotenv": "^5.2",
        "willdurand/email-reply-parser": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
